{
  "name": "@dwidge/crud-api-react",
  "version": "0.0.5",
  "description": "A CRUD API library with swr for React applications.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/crud-api-react.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/*/",
    "src"
  ],
  "scripts": {
    "prepare": "rimraf dist && ncp assets/ dist/ && tsc -b tsconfig*",
    "test": "node --test"
  },
  "peerDependencies": {
    "@dwidge/query-axios-zod": "0.0.5",
    "axios": "^1.7.7",
    "react": "^18.2",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "ncp": "^2.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=20"
  }
}
